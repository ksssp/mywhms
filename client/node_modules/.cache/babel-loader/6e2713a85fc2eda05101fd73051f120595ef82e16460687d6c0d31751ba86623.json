Village\" },\n                { data: \"creationDate\", title: \"Creation Date\", render : function(creationDate) { return (new Date(creationDate)).toLocaleDateString();} }\n            ],\n            fieldDefs: [],\n            items: null\n        };\n    },\n    async created() {\n        getAllTrademarks().then(response => {\n            this.items = response;\n            this.totalRows = this.items.length;\n        }).catch(error => {\n            console.log(error);\n        });\n    }\n}\n</script>"],"mappings":"AAwCA,SAAAA,gBAAA;AAEA,OAAAC,YAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,IAAA,WAAAA,CAAA;IACA;MACAC,SAAA;MACAC,eAAA;MACAC,gBAAA;MACAC,MAAA,GACA;QAAAJ,IAAA;QAAAK,KAAA;MAAA,GACA;QAAAL,IAAA;QAAAK,KAAA;MAAA,GACA;QAAAL,IAAA;QAAAK,KAAA;MAAA,GACA;QAAAL,IAAA;QAAAK,KAAA;MAAA,GACA;QAAAL,IAAA;QAAAK,KAAA;QAAAC,MAAA,WAAAA,CAAAC,YAAA;UAAA,WAAAC,IAAA,CAAAD,YAAA,EAAAE,kBAAA;QAAA;MAAA,EACA;MACAC,SAAA;MACAC,KAAA;IACA;EACA;EACA,MAAAC,QAAA;IACAhB,gBAAA,GAAAiB,IAAA,CAAAC,QAAA;MACA,KAAAH,KAAA,GAAAG,QAAA;MACA,KAAAb,SAAA,QAAAU,KAAA,CAAAI,MAAA;IACA,GAAAC,KAAA,CAAAC,KAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}